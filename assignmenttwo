req_file <- read_html("https://en.wikipedia.org/wiki/Indiaâ€“Pakistan_cricket_rivalry")
View(req_file)
tables<-html_nodes(req_file, "table")
tablenew <- html_table(tables[3], fill = TRUE)
View(tablenew)
table1 <- as.data.frame(tablenew)
View(table1)
counter=0
for(i in 1:nrow(table1))
{
  counter <- counter+table1$India[i]
}

print(counter)
csvfle <- read.csv("/Users/Abhishek/Desktop/AMMA 2017/Code/Code_2017/Ind_Pak_rivalry.csv")
View(csvfle)
counter<- 0
match_df<- data.frame(csvfle)
View(match_df)
for(i in 1:nrow(match_df))
{
  counter<- counter + match_df$IND[i]
}
print(counter)
